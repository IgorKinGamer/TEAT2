package br.ufsc.ine.leb.sistemaBancario.testes;

import org.hamcrest.*;

import br.ufsc.ine.leb.sistemaBancario.*;

public class CombinadorConta extends BaseMatcher<Conta>
{
	private final String id, titular, banco, agência;
	
	public CombinadorConta(String id, String titular, String banco, String agência)
	{
		// TODO Auto-generated constructor stub
		Conta c = new SistemaBancario().criarBanco(null, null).criarAgencia(null).criarConta(null);
		this.id = id;
		this.titular = titular;
		this.banco = banco;
		this.agência = agência;
	}
	
	@Override
	public boolean matches(Object obj)
	{
		if (!(obj instanceof Conta))
			return false;
		Conta c = (Conta) obj;
		
		return ;
	}

	@Override
	public void describeTo(Description arg0)
	{
		// TODO Auto-generated method stub

	}

}
