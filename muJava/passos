# Baixar
wget https://cs.gmu.edu/~offutt/mujava/{mujava.jar,openjava.jar,mujava.config} -P $muJava

# Colocar no classpath
export CLASSPATH=$CLASSPATH:$muJava/mujava.jar:$muJava/openjava.jar:$JAVA_HOME/lib/tools.jar

